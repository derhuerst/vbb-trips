{
	"name":              "vbb-trips",
	"description":       "When do trains run where in VBB?",
	"version":           "0.4.0",
	"main":              "index.js",
	"files":             ["index.js", "data/*"],
	"keywords": [
		"vbb", "berlin", "brandenburg", "public transport",
		"lines", "trips", "journeys", "date", "time",
		"open data", "json", "ndjson", "gtfs"
	],
	"author":            "Jannis R <mail@jannisr.de>",
	"homepage":          "https://github.com/derhuerst/vbb-trips",
	"repository":        "git://github.com/derhuerst/vbb-trips.git",
	"bugs":              "https://github.com/derhuerst/vbb-trips/issues",
	"license":           "ISC",
	"engines" :          {"node": ">=4"},
	"dependencies": {
		"ndjson":        "^1.4.3",
		"stream-filter": "^1",
		"through2-map": "^2"
	},
	"devDependencies": {
		"so":            "^1.0.1",
		"got": "^6.6.3",
		"key-map":       "^0.1",
		"shorthash":     "^0.0.2",
		"lodash.pick":   "^4.2",
		"array-equal":   "^1",
		"fs-promise":    "^0.5",
		"moment":        "^2.13",
		"csv-parser": "^1.11",
		"tape": "^4.6",
		"tape-catch":    "^1.0.4",
		"tap-min":       "^1.1"
	},
	"scripts": {
		"download": "curl -L 'https://github.com/derhuerst/vbb-gtfs/archive/master.zip' | tar -xf- --strip-components 1 build/data/",
		"cleanup": "rm build/data/*.txt",
		"build": "npm run download && node build/index.js && npm run cleanup",
		"test": "tape test.js | tap-min",
		"prepublish": "npm run build && npm test"
	}
}
